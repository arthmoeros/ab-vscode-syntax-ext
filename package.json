{
    "name": "artifacter-template",
    "displayName": "Artifacter Template",
    "description": "Template Format for Artifacter's atmpl files",
    "version": "0.0.1",
    "publisher": "arthmoeros",
    "engines": {
        "vscode": "^1.11.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "atmpl",
            "aliases": ["Artifacter Template", "atmpl"],
            "extensions": [".atmpl"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "atmpl",
            "scopeName": "source.atmpl",
            "path": "./syntaxes/atmpl.tmLanguage.json"
        }]
    }
}